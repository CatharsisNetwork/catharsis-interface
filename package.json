    "start": "cd webpack && webpack serve --mode production --config local.ts",
    "lint": "npx eslint --ext .ts,.tsx",
    "lint:fix": "npx eslint --ext .ts,.tsx --fix ./",
    "test": "npx jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
